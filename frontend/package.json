{
    "name": "vite-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "precommit": "lint-staged",
        "prepare": "husky install"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
